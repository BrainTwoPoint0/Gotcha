{
  "name": "gotcha-feedback",
  "version": "1.0.9",
  "description": "Developer-first contextual feedback SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "feedback",
    "react",
    "component",
    "user-feedback",
    "product-feedback"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrainTwoPoint0/Gotcha",
    "directory": "packages/sdk"
  },
  "homepage": "https://gotcha.cx"
}
